      <scriptlet id="97713a59-45a5-4b14-91b0-d1e201315b06">
        <annotation></annotation>
        <name>Step Loop in Flow Check Status (fc99b635-5c99-4493-a46f-860921437531)</name>
        <script>try {
	var current = parseInt(Iteration);
	var max = parseInt(Max);

	if(powerState == &quot;poweredOn&quot;) {
			scriptletResponse = powerState;
	}
	else if(powerState != &quot;poweredOn&quot; &amp;&amp; current &lt;= max) {
		scriptletResponse = &quot;sleep&quot;;
		current++;
		scriptletContext.put(&quot;Iteration&quot;,current);	
	}
	else {
		scriptletContext.put(&quot;Iteration&quot;,current);	
		scriptletResponse = powerState;
	}


}
catch(error) {
	scriptletContext.put(&quot;FailureMessage&quot;,error);
	scriptletResponse = &quot;failure&quot;;
}
</script>
        <type>NASHORN</type>
      </scriptlet>
