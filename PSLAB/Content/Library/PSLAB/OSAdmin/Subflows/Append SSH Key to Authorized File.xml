      <scriptlet id="e975fe31-2f93-4afa-a9fb-7fd1138f0943">
        <annotation></annotation>
        <name>Step Loop Timeout in Flow Append SSH Key to Authorized File (b7c7bbe3-1828-46ea-bbbc-6e3532506225)</name>
        <script>try {
	var current = parseInt(Iteration);
	var max = parseInt(Max);

    if(current &lt;= max) {
		scriptletResponse = &quot;sleep&quot;;
		current++;
		scriptletContext.put(&quot;Iteration&quot;,current);	
	}
	else {
		throw new Error(&quot;Appending SSH Key to Authorized Keys failed&quot;);
	}
}
catch(error) {
	scriptletContext.put(&quot;FailureMessage&quot;,error);
	scriptletResponse = &quot;timeout&quot;;
}

</script>
