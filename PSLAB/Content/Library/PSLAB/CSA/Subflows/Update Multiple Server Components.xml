        <script>try {
	var slist = JSON.parse(serverList);
	var servers = Object.keys(slist);
	scriptletContext.put(&quot;servers&quot;,JSON.stringify(servers));
	scriptletResponse = &quot;success&quot;;
}
catch(error) {
