      <scriptlet id="a7a383e6-77e6-445f-8c27-3ffd5b02657f">
        <annotation></annotation>
        <name>Step Set IP Sourcing in Flow CMT - IP Sourcing Configuration (2d14ad94-6999-4d4f-a7cc-ba1a91881731)</name>
        <script>var pr = scriptletContext.get(&quot;RS_Previous_Response&quot;);

switch(pr) {
	case &quot;more items&quot;:
		if(output.trim().length &gt; 0) {
			// customspecification
			var customSpecification = output;
			scriptletContext.put(&quot;customSpecification&quot;,customSpecification);
		}
		scriptletResponse = &quot;more&quot;;
	break;
	
	case &quot;no more items&quot;:
		var resourcing = scriptletContext.get(&quot;resourcing&quot;);
		var customSpecification = scriptletContext.get(&quot;customSpecification&quot;);
		if(resourcing != undefined &amp;&amp; customSpecification != undefined) {
		    var ipPool = {};
			if(fixedPool.trim() != &quot;&quot; &amp;&amp; fixedPool.trim().match(/^[1-9]$/) != null) {
				for(i=1;i&lt;=parseInt(fixedPool.trim());i++) {
					var tmp = {};
					ipPool[&quot;ip&quot;+i] = tmp;
				}		
			}
			scriptletContext.put(&quot;ipPool&quot;,JSON.stringify(ipPool));
			var sl = JSON.parse(serverList);
			var srvs = Object.keys(sl);
			srvs.forEach(function(i) {
				sl[i][&apos;resourcing&apos;] = resourcing;
				sl[i][&apos;customSpecification&apos;] = customSpecification;
			});
			scriptletResponse = &quot;complete&quot;;
			scriptletContext.put(&quot;serverList&quot;,JSON.stringify(sl,null,2));
		}
		else {
			scriptletResponse = &quot;failure&quot;;
			scriptletContext.put(&quot;FailureMessage&quot;,&quot;Empty IP Resourcing configuration for CMT&quot;);
		}
	break;
	
	case udefined:
		scriptletResponse = &quot;failure&quot;;
		scriptletContext.put(&quot;FailureMessage&quot;,&quot;Datatabse Error:&quot; + output);
	break;
}
</script>
        <type>NASHORN</type>
